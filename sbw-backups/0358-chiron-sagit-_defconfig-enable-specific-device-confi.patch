From: Demon Singur <demonsingur@gmail.com>
Date: Sat, 19 May 2018 14:59:03 +0000
Subject: [PATCH 358/420] {chiron,sagit}_defconfig: enable specific device
 config

Change-Id: I069d2a8e0f912abd6af95696e829ed27c80b7b49
---
 arch/arm64/configs/chiron_defconfig | 13 ++++++++-----
 arch/arm64/configs/sagit_defconfig  | 13 ++++++++-----
 2 files changed, 16 insertions(+), 10 deletions(-)

diff --git a/arch/arm64/configs/chiron_defconfig b/arch/arm64/configs/chiron_defconfig
index 4349c0e6982c..2d26e1c11d2f 100644
--- a/arch/arm64/configs/chiron_defconfig
+++ b/arch/arm64/configs/chiron_defconfig
@@ -347,14 +347,15 @@ CONFIG_INPUT_KEYRESET=y
 CONFIG_KEYBOARD_GPIO=y
 # CONFIG_INPUT_MOUSE is not set
 CONFIG_INPUT_TOUCHSCREEN=y
-CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE_v21=y
-CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE_v21=y
-CONFIG_SECURE_TOUCH=y
-CONFIG_TOUCHSCREEN_ST=y
-CONFIG_TOUCHSCREEN_ST_I2C=y
+CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE_FORCE=y
+CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_RMI_DEV_FORCE=y
 CONFIG_INPUT_MISC=y
 CONFIG_INPUT_QPNP_POWER_ON=y
 CONFIG_INPUT_UINPUT=y
+CONFIG_INPUT_STMVL53L0=y
+CONFIG_INPUT_FINGERPRINT=y
+CONFIG_FINGERPRINT_GOODIX_TA=y
+CONFIG_FINGERPRINT_FPC1268_TEE=y
 # CONFIG_SERIO is not set
 # CONFIG_VT is not set
 # CONFIG_LEGACY_PTYS is not set
@@ -420,6 +421,7 @@ CONFIG_MEDIA_CAMERA_SUPPORT=y
 CONFIG_MEDIA_CONTROLLER=y
 CONFIG_VIDEO_V4L2_SUBDEV_API=y
 CONFIG_VIDEO_FIXED_MINOR_RANGES=y
+CONFIG_SPI_PEELIR=y
 CONFIG_V4L_PLATFORM_DRIVERS=y
 CONFIG_MSM_CAMERA=y
 CONFIG_MSMB_CAMERA=y
@@ -531,6 +533,7 @@ CONFIG_MSM_MHI=y
 CONFIG_MSM_MHI_UCI=y
 # CONFIG_MSM_11AD is not set
 CONFIG_USB_BAM=y
+CONFIG_MACH_CHIRON=y
 CONFIG_MSM_MDSS_PLL=y
 CONFIG_REMOTE_SPINLOCK_MSM=y
 CONFIG_MSM_TIMER_LEAP=y
diff --git a/arch/arm64/configs/sagit_defconfig b/arch/arm64/configs/sagit_defconfig
index 4349c0e6982c..30a10d6f863b 100644
--- a/arch/arm64/configs/sagit_defconfig
+++ b/arch/arm64/configs/sagit_defconfig
@@ -347,14 +347,15 @@ CONFIG_INPUT_KEYRESET=y
 CONFIG_KEYBOARD_GPIO=y
 # CONFIG_INPUT_MOUSE is not set
 CONFIG_INPUT_TOUCHSCREEN=y
-CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE_v21=y
-CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE_v21=y
-CONFIG_SECURE_TOUCH=y
-CONFIG_TOUCHSCREEN_ST=y
-CONFIG_TOUCHSCREEN_ST_I2C=y
+CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE_FORCE=y
+CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_RMI_DEV_FORCE=y
 CONFIG_INPUT_MISC=y
 CONFIG_INPUT_QPNP_POWER_ON=y
 CONFIG_INPUT_UINPUT=y
+CONFIG_INPUT_STMVL53L0=y
+CONFIG_INPUT_FINGERPRINT=y
+CONFIG_FINGERPRINT_GOODIX_TA=y
+CONFIG_FINGERPRINT_FPC1268_TEE=y
 # CONFIG_SERIO is not set
 # CONFIG_VT is not set
 # CONFIG_LEGACY_PTYS is not set
@@ -420,6 +421,7 @@ CONFIG_MEDIA_CAMERA_SUPPORT=y
 CONFIG_MEDIA_CONTROLLER=y
 CONFIG_VIDEO_V4L2_SUBDEV_API=y
 CONFIG_VIDEO_FIXED_MINOR_RANGES=y
+CONFIG_SPI_PEELIR=y
 CONFIG_V4L_PLATFORM_DRIVERS=y
 CONFIG_MSM_CAMERA=y
 CONFIG_MSMB_CAMERA=y
@@ -531,6 +533,7 @@ CONFIG_MSM_MHI=y
 CONFIG_MSM_MHI_UCI=y
 # CONFIG_MSM_11AD is not set
 CONFIG_USB_BAM=y
+CONFIG_MACH_SAGIT=y
 CONFIG_MSM_MDSS_PLL=y
 CONFIG_REMOTE_SPINLOCK_MSM=y
